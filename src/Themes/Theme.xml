<?xml version="1.0" encoding="utf-8" ?>

<theme title="Default" description="">
	<cultures>
		<culture name="zh-CN">
			<property name="" value="" />
		</culture>
	</cultures>

	<component name="jquery">
		<file name="/scripts/jquery.js" />
		<file name="/scripts/jquery-ui.js" />
		<file name="/scripts/jquery.form.js" />
		<file name="/scripts/jquery.validate.js" />
	</component>

	<component name="bootstrap">
		<file name="http://netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" />
		<file name="http://netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js" />
	</component>

	<component name="layout">
		<dependencies>
			<dependency name="jquery" />
		</dependencies>

		<file name="reset.css" />
		<file name="layout.css" />
		<file name="layout.js" />
	</component>

	<component name="dialog">
		<dependencies>
			<dependency name="jquery" />
		</dependencies>

		<file name="dialog.js" />
	</component>

	<include name="jquery" />
	<include name="bootstrap" />
	<include name="layout" />
	<include name="dialog" />
</theme>